<!DOCTYPE module SYSTEM "module.dtd">
<module name="eff_scheduling_colibri" dir="ctrl">
  <doc>
    <description>Interpolation of control effectivenss matrix of the Colibri.</description>
  </doc>
 
  <header>
    <file name="eff_scheduling_colibri.h"/>
  </header>
  <init fun="ctrl_eff_scheduling_init()"/>
  <periodic fun="ctrl_eff_scheduling_periodic()" freq="20.0"/>
  <periodic fun="ctrl_eff_scheduling_report()" freq="0.5" autorun="FALSE"/> <!-- TRUE  FALSE-->
  <makefile target="ap|nps" firmware="rotorcraft" >
    <file name="eff_scheduling_colibri.c"/>
  </makefile>
</module>
